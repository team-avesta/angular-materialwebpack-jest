{
	"name": "<%- props.appName %>",
	"version": "0.1.0",
	"description": "Angular Webpack ES6 application generated by generator-angular-webpack-es6",
	"homepage": "",
	"bin":
	{
		"cross-env": "./node_modules/cross-env/bin/cross-env.js",
		"webpack": "./node_modules/webpack/bin/webpack.js",
		"webpack-dev-server": "./node_modules/webpack-dev-server/bin/webpack-dev-server.js"
	},
	"scripts":
	{
		"start": "npm run dev",
		"dev": "cross-env NODE_ENV=development webpack-dev-server --progress --bail --open",
		"build": "cross-env NODE_ENV=production webpack -p --colors",
		"test": "cross-env NODE_ENV=development jest",
		"test:live": "cross-env NODE_ENV=development jest --watchAll",
		"eslint": "eslint ./src/app/**",
		"precommit": "npm test"
	},
	"jest":
	{
		"roots": [
			"src/app"
		],
		"setupTestFrameworkScriptFile": "./config/jest.init.js",
		"transform":
		{
			"^.+\\.(js|jsx)$": "/home/vinaynb/Desktop/Projects/RND/temp-angular-es6/node_modules/babel-jest"
		},
		"moduleNameMapper":
		{
			"\\.(html|scss|ico|jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/config/fileMock.js",
			"\\.(css|less)$": "<rootDir>/config/styleMock.js",
			"^_appRoot(.*)$": "<rootDir>/src/app/$1",
			"^_images(.*)$": "<rootDir>/src/assets/images/$1",
			"^_stylesheets(.*)$": "<rootDir>/src/assets/styles/$1",
			"^_scripts(.*)$": "<rootDir>/src/assets/js/$1"
		}
	},
	"dependencies":
	{
		"@uirouter/angularjs": "^1.0.11",
		"angular": "^1.6.6",
		"ng-storage": "^0.3.2",
		"angular-aria": "^1.6.6",
		"v-accordion": "^1.6.0"
	},
	"devDependencies":
	{
		"angular-mocks": "^1.6.6",
		"assets-webpack-plugin": "^3.4.0",
		"autoprefixer": "^6.5.0",
		"babel-core": "^6.14.0",
		"babel-loader": "^6.2.5",
		"babel-plugin-angularjs-annotate": "^0.5.2",
		"babel-plugin-module-resolver": "^3.0.0",
		"babel-preset-env": "^1.6.1",
		"baggage-loader": "^0.2.4",
		"clean-webpack-plugin": "^0.1.10",
		"cross-env": "^3.0.0",
		"css-loader": "^0.25.0",
		"es6-promise": "^4.0.3",
		"eslint": "^4.9.0",
		"eslint-loader": "^1.5.0",
		"eslint-plugin-angular": "^3.1.1",
		"extract-text-webpack-plugin": "^2.0.0-beta.4",
		"file-loader": "^0.9.0",
		"html-loader": "^0.4.4",
		"html-webpack-plugin": "^2.22.0",
		"imagemin-pngquant": "^5.0.0",
		"manifest-revision-webpack-plugin": "^0.3.0",
		"ngtemplate-loader": "^1.3.1",
		"node-sass": "^3.10.0",
		"jest": "^21.2.1",
		"postcss-loader": "^0.13.0",
		"require-hacker": "^3.0.1",
		"sass-loader": "^4.0.2",
		"style-loader": "^0.13.1",
		"url-loader": "^0.5.7",
		"webpack": "^3.5.0",
		"webpack-dev-server": "^2.2.0"
	},
	"engines":
